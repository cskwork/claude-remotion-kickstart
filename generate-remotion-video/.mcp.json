{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "@playwright/mcp@latest", "--save-video=1280x720"],
      "env": {}
    },
    "remotion-documentation": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "@remotion/mcp@latest"],
      "env": {}
    },
    "elevenlabs": {
      "type": "stdio",
      "command": "uvx",
      "args": ["elevenlabs-mcp"],
      "env": {}
    },
    "replicate": {
      "type": "sse",
      "url": "https://mcp.replicate.com/sse"
    }
  }
}

